{
  "name": "@leonardodino/react-native-datepicker",
  "version": "1.8.0",
  "description": "react-native DatePicker component Android and iOS",
  "main": "DatePicker.js",
  "scripts": {
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "np": "np --no-yarn --no-cleanup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardodino/react-native-datepicker.git"
  },
  "keywords": [
    "react-native",
    "DatePicker"
  ],
  "author": "leonardodino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonardodino/react-native-datepicker/issues"
  },
  "homepage": "https://github.com/leonardodino/react-native-datepicker#readme",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "coveralls": "^2.11.9",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "husky": "^0.14.3",
    "inquirer": "^3.0.5",
    "jest": "^21.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^5.0.0",
    "np": "^2.17.0",
    "prettier": "1.8.2",
    "react": "16.0.0-beta.5",
    "react-dom": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-test-renderer": "16.0.0-beta.5"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "example"
    ],
    "testPathIgnorePatterns": [
      "example",
      "/node_modules/",
      "tools/"
    ]
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --no-bracket-spacing --print-width 100 --no-prose-wrap --no-semi --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "dependencies": {
    "intl": "^1.2.5"
  }
}
